{
  "name": "northwind-product-react",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@babel/register": "^7.0.0",
    "axios": "0.18.0",
    "dyson": "^2.0.2",
    "json-server": "^0.14.0",
    "lodash": "4.17.5",
    "odata": "^0.4.0",
    "odata-client": "^0.6.9",
    "react": "16.2.0",
    "react-dev-utils": "5.0.0",
    "react-dom": "16.2.0",
    "react-redux": "5.0.7",
    "redux": "3.7.2",
    "redux-thunk": "2.2.0",
    "yargs": "^9.0.1",
    "body-parser": "^1.13.3",
    "cookie-parser": "~1.3.5",
    "cross-env": "^5.1.5",
    "debug": "^4.0.1",
    "express": "^4.13.4",
    "fresh": "^0.5.2",
    "glob": "^5.0.14",
    "jade": "~1.11.0",
    "negotiator": "^0.6.1",
    "serve-favicon": "~2.3.0"
  },
  "devDependencies": {
    "@types/chai": "^4.1.3",
    "@types/express": "^4.0.36",
    "@types/lodash": "^4.14.68",
    "@types/mocha": "^5.2.0",
    "@types/mongoose": "^4.7.18",
    "@types/node": "^8.0.9",
    "@types/sinon": "^4.3.3",
    "nodemon": "^1.17.4",
    "sinon": "^5.0.7",
    "ts-node": "^6.0.3",
    "typescript": "^2.4.1",
    "autoprefixer": "7.1.6",
    "babel-core": "^6.2.1",
    "babel-jest": "^21.2.0",
    "babel-loader": "^6.2.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-react": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "1.1.3",
    "css-loader": "0.28.7",
    "dotenv": "4.0.0",
    "dotenv-expand": "4.2.0",
    "enzyme": "^3.0.0",
    "enzyme-adapter-react-16": "^1.0.0",
    "eslint": "^4.13.0",
    "eslint-plugin-react": "^7.4.0",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.5",
    "fs-extra": "3.0.1",
    "html-webpack-plugin": "2.29.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^21.2.1",
    "jest-css-modules-transform": "^2.0.0",
    "jest-html-reporter": "^2.4.2",
    "jsdom": "^11.12.0",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "2.0.8",
    "promise": "8.0.1",
    "react-test-renderer": "^16.0.0",
    "style-loader": "0.19.0",
    "url-loader": "0.6.2",
    "webpack": "^3.8.1",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^2.9.4"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.{js,jsx}",
      "!**/node_modules/**",
      "!**/vendor/**"
    ],
    "rootDir": ".",
    "testRegex": "/src/test/.*test\\.js$",
    "setupFiles": [
      "<rootDir>/src/test/polyfills.js",
      "<rootDir>/src/test/setup.js"
    ],
    "moduleNameMapper": {
      "\\.(css|less)$": "identity-obj-proxy"
    },
    "testResultsProcessor": "./node_modules/jest-html-reporter"
  },
  "jest-html-reporter": {
    "pageTitle": "Northwind products",
    "outputPath": "test-report.html",
    "includeFailureMsg": true
  },
  "scripts": {
    "test": "jest",
    "debug": "node --inspect-brk=5858 webpack.config.js",
    "build": "node ./node_modules/webpack/bin/webpack.js",
    "serve": "node ./node_modules/webpack-dev-server/bin/webpack-dev-server.js",
    "build:serve": "npm run build && npm run serve",
    "json:server:static":"json-server --watch ./mocks/db.json --port 3004",
    "json:server:dynamic":"json-server --watch ./mocks/json-server.js --port 3004",
    "dyson":"dyson ./mocks 3004",
    "express:serve": "node dist-ts/index.js",
    "express:build": "tsc -p server",
    "express:build:serve": "npm run express:build && npm run express:build"
  }
}
